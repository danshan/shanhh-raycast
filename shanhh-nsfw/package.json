{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "shanhh-nsfw",
  "title": "shanhh-nsfw",
  "description": "ShanHH NSFW toolbox.",
  "icon": "command-icon.png",
  "author": "danshan",
  "owner": "shanhh",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "ShanHH NSFW",
      "description": "ShanHH NSFW toolbox.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "btsowHost",
      "type": "textfield",
      "title": "Btsow Site",
      "description": "input site name, all available sites are listed below or check in https://tellme.pw/btsow.",
      "placeholder": "e.g. https://btsow.com/",
      "link": "https://tellme.pw/btsow",
      "required": true,
      "default": "https://btsow.motorcycles"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.74.1",
    "@raycast/utils": "^1.15.0",
    "crypto-js": "^4.1.1",
    "got": "^14.2.1",
    "node-html-parser": "^6.1.13",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/crypto-js": "^4.2.2",
    "@types/got": "^9.6.12",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
