{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "shanhh-toolbox",
  "title": "ShanHH Toolbox",
  "description": "ShanHH Toolbox",
  "icon": "command-icon.png",
  "author": "danshan",
  "owner": "shanhh",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "ShanHH Toolbox",
      "description": "Toolbox for ShanHH",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "authFile",
      "description": "TOtp Auth File",
      "type": "file",
      "required": true,
      "title": "TOTP Auth File"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.74.1",
    "got": "^14.2.1",
    "ini": "^4.1.2",
    "ip": "^2.0.1",
    "otpauth": "^9.2.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/got": "^9.6.12",
    "@types/ini": "^4.1.0",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "@types/ip": "^1.1.3",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
